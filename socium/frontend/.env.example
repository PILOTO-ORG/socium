# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# Environment
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_AI_ANALYSIS=true
REACT_APP_ENABLE_CSV_UPLOAD=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=false

# Analytics (exemplo para futuras integrações)
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# Social Login (para futuras funcionalidades)
# REACT_APP_LINKEDIN_CLIENT_ID=your-linkedin-client-id
# REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# UI Configuration
REACT_APP_APP_NAME=Socium AI
REACT_APP_APP_VERSION=1.0.0
REACT_APP_COMPANY_NAME=Socium Tech

# Debug
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=warn

# Build Configuration
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=false
