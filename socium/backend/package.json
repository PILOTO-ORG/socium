{
  "name": "socium-backend",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "start:dev": "ts-node src/app.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp -r src/database/migrations dist/database/ 2>/dev/null || :"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.8.0",
    "sequelize": "^6.37.7",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.32.0",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
